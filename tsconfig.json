{
    "extends": "@docusaurus/tsconfig",
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@/*": ["src/*"],
            "@components/*": ["src/components/*"],
            "@pages/*": ["src/pages/*"],
            "@theme/*": ["src/theme/*"],
            "@css/*": ["src/css/*"]
        }
    }
}
